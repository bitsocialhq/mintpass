{
  "name": "@mintpass/challenge",
  "version": "1.1.8",
  "description": "MintPass pkc-js challenge implementation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "test": "yarn build && ./scripts/test-with-node.sh",
    "test:manual": "yarn build && cd ../contracts && npx hardhat test ../challenge/test/mintpass-integration.test.js --network localhost",
    "clean": "rm -rf dist/",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release-notes": "conventional-changelog -p angular -r 1 > RELEASE_NOTES.md"
  },
  "dependencies": {
    "@keyv/sqlite": "^4.0.5",
    "env-paths": "^3.0.0",
    "keyv": "5.5.0",
    "multiformats": "^13.1.0",
    "uint8arrays": "^4.0.3",
    "viem": "2.0.0"
  },
  "peerDependencies": {
    "@plebbit/plebbit-js": ">=0.0.0"
  },
  "devDependencies": {
    "@plebbit/plebbit-js": "https://github.com/plebbit/plebbit-js.git#a21d896bd55c76070758dae65dc0693d1c3726db",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.7.8",
    "chai": "^4.2.0",
    "conventional-changelog-cli": "^5.0.0",
    "ethers": "6.4.0",
    "hardhat": "^2.19.0",
    "kubo": "0.36.0",
    "tempy": "^3.1.0",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "bitsocial",
    "pkc",
    "challenge",
    "nft",
    "mintpass",
    "authentication"
  ],
  "author": "Bitsocial Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitsocialhq/mintpass",
    "directory": "challenge"
  }
}
